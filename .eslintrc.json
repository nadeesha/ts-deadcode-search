{
  "extends": [
    "eslint:recommended",
    "prettier",
    "prettier/@typescript-eslint"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  },
  "rules": {
    "no-undef": 0,
    "no-unused-vars": 0,
    "no-useless-constructor": 0
  }
}
